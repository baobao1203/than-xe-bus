<launch>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find mobile_robot)/world/my_world.sdf"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>


    <!-- Load mô hình URDF vào Gazebo -->
    <param name="robot_description" command="$(find xacro)/xacro $(find mobile_robot)/urdf/mobile_robot.urdf" />

    <!-- Spawn robot vào Gazebo -->
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" output="screen" args="-param robot_description -urdf -model mobile_robot" />


</launch>